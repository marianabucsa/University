<AnchorPane prefHeight="349.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.athleticscompetiton.controller.Controller">
   <children>
      <TableView fx:id="searchTable" layoutX="29.0" layoutY="34.0" prefHeight="267.0" prefWidth="256.0">
