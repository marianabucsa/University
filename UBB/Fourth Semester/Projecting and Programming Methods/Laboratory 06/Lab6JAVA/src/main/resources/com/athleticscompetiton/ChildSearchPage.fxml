          <TableColumn fx:id="firstnameColumn" prefWidth="75.0" text="First Name" />
          <TableColumn fx:id="lastnameColumn" prefWidth="75.0" text="Last Name" />
            <TableColumn fx:id="ageColumn" prefWidth="75.0" text="Age" />
            <TableColumn fx:id="nrOfRoundsColumn" prefWidth="75.0" text="Nr Of Enrollments" />
        </columns>
      </TableView>
      <TextField fx:id="firstnameText" layoutX="310.0" layoutY="86.0" />
      <TextField fx:id="lastnameText" layoutX="310.0" layoutY="189.0" />
      <Button layoutX="359.0" layoutY="254.0" mnemonicParsing="false" onAction="#onSearchByNameClicked" text="Search" />
      <Label layoutX="310.0" layoutY="62.0" text="First Name" />
      <Label layoutX="311.0" layoutY="166.0" text="Last Name" />
      <ComboBox fx:id="searchRound" layoutX="484.0" layoutY="86.0" onAction="#roundsComboBox" prefWidth="150.0" />
      <ComboBox fx:id="searchAgeGroups" layoutX="484.0" layoutY="189.0" onAction="#ageGroupsComboBox" prefWidth="150.0" />
      <Label layoutX="484.0" layoutY="62.0" text="Rounds" />
      <Label layoutX="484.0" layoutY="166.0" text="Age Groups" />
   </children>
</AnchorPane>
